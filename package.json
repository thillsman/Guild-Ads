{
  "name": "guild-ads",
  "private": true,
  "scripts": {
    "dev:api": "pnpm --filter @guild-ads/api dev",
    "dev:web": "pnpm --filter @guild-ads/web dev",
    "dev": "pnpm run --parallel dev:api dev:web",
    "build": "pnpm run --parallel build:api build:web",
    "build:api": "pnpm --filter @guild-ads/api build",
    "build:web": "pnpm --filter @guild-ads/web build",
    "db:migrate": "supabase db push",
    "db:reset": "supabase db reset",
    "db:types": "supabase gen types typescript --local > packages/shared/src/database.types.ts",
    "lint": "pnpm -r lint",
    "typecheck": "pnpm -r typecheck"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@9.0.0",
  "engines": {
    "node": ">=20.0.0"
  }
}
